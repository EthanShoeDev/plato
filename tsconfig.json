{
  "extends": "astro/tsconfigs/strictest",
  "exclude": ["_next_implementation/", "node_modules/", "dist/", "public/"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "exactOptionalPropertyTypes": false
  }
}
